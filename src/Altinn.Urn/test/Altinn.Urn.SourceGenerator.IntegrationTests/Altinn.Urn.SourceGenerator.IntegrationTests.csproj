<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <IsPackable>false</IsPackable>
    <IsTestProject>true</IsTestProject>
  </PropertyGroup>

  <ItemGroup>
    <SourceGenerator Include="..\..\src\Altinn.Urn.SourceGenerator\Altinn.Urn.SourceGenerator.csproj" />
    <ProjectReference Include="..\..\src\Altinn.Urn\Altinn.Urn.csproj" />
  </ItemGroup>

  <ItemGroup>
		<ProjectReference Include="@(SourceGenerator)" 
			ReferenceOutputAssembly="false" 
			Private="true"
			OutputItemType="_SourceGeneratorPath" />
	</ItemGroup>

  <Target Name="_AddSourceGeneratorReference" AfterTargets="ResolveReferences">
    <ItemGroup>
      <Analyzer Include="@(_SourceGeneratorPath)" />
    </ItemGroup>
  </Target>

  <ItemGroup>
    <Using Include="Xunit" />
  </ItemGroup>

</Project>
